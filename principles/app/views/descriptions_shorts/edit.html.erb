<h1>Editing descriptions_short</h1>

<% form_for(@descriptions_short) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label : idShort %><br />
    <%= f.text_field : idShort %>
  </p>
  <p>
    <%= f.label :descriptionShort %><br />
